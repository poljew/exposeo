<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <PackageJsonName Condition="$(PackageJsonName) == ''">exposeo-root</PackageJsonName>
    <PackageJsonVersion Condition="$(PackageJsonVersion) == ''">1.0.0</PackageJsonVersion>
    <PackageJsonPrivate Condition="$(PackageJsonPrivate) == ''">true</PackageJsonPrivate>
    <PackageJsonScriptsDev Condition="$(PackageJsonScriptsDev) == ''">concurrently "npm run dev:server" "npm run dev:client"</PackageJsonScriptsDev>
    <PackageJsonScriptsDevServer Condition="$(PackageJsonScriptsDevServer) == ''">node server.js</PackageJsonScriptsDevServer>
    <PackageJsonScriptsDevClient Condition="$(PackageJsonScriptsDevClient) == ''">npm run dev --prefix frontend</PackageJsonScriptsDevClient>
    <PackageJsonScriptsBuild Condition="$(PackageJsonScriptsBuild) == ''">npm install --prefix frontend &amp;&amp; npm run build --prefix frontend</PackageJsonScriptsBuild>
    <PackageJsonScriptsStart Condition="$(PackageJsonScriptsStart) == ''">node server.js</PackageJsonScriptsStart>
    <PackageJsonDependenciesExpress Condition="$(PackageJsonDependenciesExpress) == ''">^4.19.2</PackageJsonDependenciesExpress>
    <PackageJsonDependenciesCors Condition="$(PackageJsonDependenciesCors) == ''">^2.8.5</PackageJsonDependenciesCors>
    <PackageJsonDependenciesDotenv Condition="$(PackageJsonDependenciesDotenv) == ''">^16.4.5</PackageJsonDependenciesDotenv>
    <PackageJsonDevdependenciesConcurrently Condition="$(PackageJsonDevdependenciesConcurrently) == ''">^8.2.2</PackageJsonDevdependenciesConcurrently>
  </PropertyGroup>
</Project>